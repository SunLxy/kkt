{
  "lerna": "3.19.0",
  "version": "5.4.2",
  "npmClient": "yarn",
  "packages": [
    "packages/*"
  ],
  "command": {
    "create": {
      "homepage": "https://github.com/kktjs/kkt-next",
      "license": "MIT"
    },
    "version": {
      "allowBranch": "master",
      "conventionalCommits": true,
      "noChangelog": true,
      "exact": true,
      "forcePublish": true,
      "message": "chore(release): %s"
    }
  },
  "npmClientArgs": [
    "--no-lockfile"
  ]
}
