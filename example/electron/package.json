{
  "name": "@kkt/template-electron",
  "version": "5.11.0",
  "description": "Electron example.",
  "main": "lib/main.js",
  "private": true,
  "scripts": {
    "watch": "tsbb watch -s app --source-maps=none",
    "start": "kkt start --port 1909 --no-openBrowser",
    "build": "GENERATE_SOURCEMAP=false kkt build",
    "app": "NODE_ENV=dev ELECTRON_DISABLE_SECURITY_WARNINGS=true electron .",
    "app:build": "electron ."
  },
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "electron": "11.1.0",
    "kkt": "5.11.0",
    "tsbb": "1.7.8"
  },
  "dependencies": {
    "react": "17.0.1",
    "react-dom": "17.0.1"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
